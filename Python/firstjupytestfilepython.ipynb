   "source": [
    "def facNumbers(i):\n",
    "    if i == 0:\n",
    "        return 0\n",
    "    #somehow this works.\n",
    "    elif i == 1:\n",
    "        return 1\n",
    "    return i * facNumbers(i - 1)\n",
    "    \n",
    "facNumbers(9)"
   ]
